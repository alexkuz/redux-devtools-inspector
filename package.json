    "lodash.debounce": "^4.0.3",
    "react-base16-styling": "^0.4.1",
    "react-json-tree": "^0.10.5",
    "redux-devtools-themes": "^1.0.0"
  },
  "pre-commit": [
    "lint"
  ]
